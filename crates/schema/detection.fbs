include "trace_metadata.fbs";

namespace bridge.schema;

table BoundingBox {
    x1: float;
    y1: float;
    x2: float;
    y2: float;
    confidence: float;
    class_id: uint32;
}

table DetectionResult {
    frame_number: uint64;
    timestamp_ns: uint64;
    detections: [BoundingBox];

    trace: TraceMetadata;
}

root_type DetectionResult;
